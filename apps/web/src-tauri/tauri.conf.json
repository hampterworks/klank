{
  "$schema": "../../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "klank",
  "version": "1.0.0",
  "identifier": "klank",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "turbo run dev",
    "beforeBuildCommand": "turbo run build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "klank",
        "width": 1280,
        "height": 1024,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
